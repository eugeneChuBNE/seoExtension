{
  "manifest_version": 3,
  "name": "SEO Analyser",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    },
    "default_title": "SEO Analyser"
  },
  "content_scripts": [
    {
      "matches": ["https://vietnix.vn/*"],
      "js": ["content.js"]
    }
  ]
}
